<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vendingmachine.controller.SellerPageController">
   <children>
      <Button layoutX="26.0" layoutY="350.0" mnemonicParsing="false" onAction="#returnToMainPage" text="Back" />
      <Label layoutX="253.0" layoutY="27.0" prefHeight="49.0" prefWidth="187.0" text="Hi seller :)">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="modify_product" layoutX="111.0" layoutY="118.0" mnemonicParsing="false" onAction="#sellerModify" prefHeight="49.0" prefWidth="164.0" text="modify product " />
      <Button fx:id="report1" layoutX="111.0" layoutY="212.0" mnemonicParsing="false" onAction="#summaryAvailableProducts" prefHeight="49.0" prefWidth="392.0" text="Report for currently avaiable items" />
      <Button fx:id="listProduct" layoutX="331.0" layoutY="118.0" mnemonicParsing="false" onAction="#listProduct" prefHeight="49.0" prefWidth="172.0" text="List Product " />
      <Button fx:id="report2" layoutX="111.0" layoutY="276.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="392.0" text="Report for summary sold products" />
   </children>
</AnchorPane>
